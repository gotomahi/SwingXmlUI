<?xml version="1.0" encoding="UTF-8"?>
<page name="Prelim"  external_atts="true">
	<action>com.gui.policy.PrelimAction</action>
	<panel dimension="0,0,1,1,1.0,1.0,10,1,0,0,0,0">
		<panel border_left_title="" dimension="0,0,1,1,1.0,1.0,10,1,0,0,0,0">
			<property label="Make" type="Label" position="0,0" size="80,25" />
			<property name="treeMap.MakeModel.make" actionName="Make" type="ComboBox" list="treeMap.MakeList" default_value="--Please Select--" position="1,0" size="150,25"
				listeners="ActionListener" />
			<property label="Model" type="Label" position="0,1" size="80,25" />
			<property name="treeMap.MakeModel.model" actionName="Model" type="ComboBox" list="treeMap.ModelList" default_value="--Please Select--" position="1,1" size="150,25"
				listeners="ActionListener" />
			<property label="Class" type="Label" position="0,2" size="80,25" />
			<property name="treeMap.MakeModel.clss" actionName="Class" type="ComboBox" list="treeMap.ClassList" default_value="--Please Select--" position="1,2" size="150,25" />
			<property label="Sold Date" type="Label" position="0,3" size="150,25" />
			<property name="treeMap.policyMap.vehicle.soldDate" type="DatePicker" position="1,3" size="150,25" />
			<property label="Reg Date" type="Label" position="0,4" size="80,25" />
			<property name="treeMap.policyMap.vehicle.regDate" type="DatePicker" position="1,4" size="150,25" />
			<property label="Policy Sold Date" type="Label" position="0,5" size="80,25" />
			<property name="treeMap.policyMap.policySoldDate" type="DatePicker" position="1,5" size="150,25" />
			<property label="Dealer" type="Label" position="0,6" size="80,25" />
			<property name="treeMap.policyMap.dealer" type="ComboBox" list="treeMap.DealerList" labelProperty="dealerName" default_value="--Please Select--" position="1,6" size="150,25" />
			<property label="Employee" type="Label" position="0,7" size="80,25" />
			<property name="treeMap.policyMap.employee" type="TextField" position="1,7" size="150,25" />
			<panel position="0,8">
				<property name="Show Products" inpageaction="true" default="true" type="Button" position="0,0" size="80,25" />
			</panel>
		</panel>
		<panel border_left_title="" dimension="1,0,1,1,1.0,1.0,10,1,0,0,0,0">
			<PanelComponent dimension="0,0,1,1,1.0,1.0,18,1,0,0,0,0" list="treeMap.products" type="PanelComponent" name="${productType}" border_left_title="">
				<panel dimension="0,0,1,1,1.0,1.0,18,1,0,0,0,0">
					<property name="treeMap.products.${productType}.productType" type="CheckBox" position="0,0" size="20,25" />
					<property label="productType" type="Label" position="1,0" size="80,25" />
					<property name="treeMap.products.${productType}.product" type="ComboBox" list="treeMap.products.${productType}.ProductList" labelProperty="productName" default_value="--Please Select--"
						position="2,0" size="250,25" listeners="ActionListener" actionName="Product" />
				</panel>
				<panel dimension="0,1,1,1,1.0,1.0,18,1,0,0,0,0">
					<property label="Cover Type" type="Label" position="0,0" size="80,25" />
					<property name="treeMap.products.${productType}.coverType" type="ComboBox" list="treeMap.products.${productType}.CoverTypeList" default_value="--Please Select--"
						position="1,0" size="150,25" actionName="CoverType" />
					<property label="Policy Term" type="Label" position="0,1" size="80,25" />
					<property name="treeMap.products.${productType}.coverPeriod" type="ComboBox" list="treeMap.products.${productType}.TermList" default_value="--Please Select--" position="1,1"
						size="150,25" listeners="ActionListener" actionName="Term"/>
					<property label="Policy Start Date" type="Label" position="0,2" size="80,25" />
					<property name="treeMap.products.${productType}.startDate" type="DatePicker" position="1,2" size="150,25" />
					<property label="Policy Amount" type="Label" position="0,3" size="80,25" />
					<property name="treeMap.products.${productType}.coverAmount" type="TextField" position="1,3" size="150,25" />
				</panel>
			</PanelComponent>
			<panel dimension="0,1,1,1,1.0,1.0,12,1,0,0,0,0">
				<property label="Policy Amount" type="Label" position="0,0" size="80,25" />
				<property name="treeMap.planAmount" type="TextField" position="1,0" size="250,25" />
			</panel>
		</panel>
	</panel>
</page>
