<?xml version="1.0" encoding="UTF-8"?>
<page name="Policy">
	<action>com.gui.policy.PlanAction</action>
	<panel treepane="true" divider_location="220" dimension="0,0,1,1,1.0,1.0,18,1,0,0,0,0">
		<node name="Plan" next_page="Prelim">
			<node name="Vehicle" next_page="Vehicle" image="gui/images/vehicle.png" />
			<node name="Customer" next_page="Customer" image="gui/images/customer.png" />
			<node name="Documents" next_page="PolicyDocuments" image="gui/images/documents.png" />
			<node name="Notes" next_page="PolicyNotes" image="gui/images/notes.png" />
			<node name="Claims" next_page="Claims" image="gui/images/claims.png" enable_expr="${policyMap.claims} > 0">
				<node name="claimNumber" sub_tree_data="policyMap.claims" next_page="ClaimDetails">
					<node name="Documents" next_page="ClaimDocuments" image="gui/images/documents.png" />
					<node name="Notes" next_page="ClaimNotes" image="gui/images/notes.png" />
				</node>
			</node>
			<action name="Save" image="gui/images/save.png" description="Save" next_page="PolicySummary" />
			<action name="Transfer" image="gui/images/transfer.png" description="Transfer" enable_expr="${operation}=amendment" />
			<action name="Cancel" image="gui/images/cancel.png" description="Cancel" enable_expr="${operation}=amendment" />
			<action name="Authorization" image="gui/images/authorization.ico" description="Authorization" enable_expr="${operation}=amendment" />
			<action name="Close" image="gui/images/delete.png" description="Close" />
		</node>
	</panel>
</page>
