<?xml version="1.0" encoding="UTF-8"?>
<policy-atts>
	<atts name="prelim">
		<attribute name="plan.policy.make" type="select" data="MakeList" displayorder="1" script="onChange='getModels(this.value)'" label="policy.make" id="make"/>
		<attribute name="plan.policy.model" type="select" data="ModelList" displayorder="2" script="onChange='getClasses(document.getElementById('make').value,this.value)'"  label="policy.model" id="models"/>
		<attribute name="plan.policy.class" type="select" data="ClassList" displayorder="3"  label="policy.class" id="classes"/>
		<attribute name="plan.policy.vehicleSoldDate" type="date" displayorder="4"  label="policy.vehicleSoldDate" id="datepicker1"/>
		<attribute name="plan.policy.policySoldDate" type="date" displayorder="5" label="policy.policySoldDate" id="datepicker2"/>
		<attribute name="plan.policy.mileage" type="textfield" displayorder="6"  label="policy.mileage"/>
		<attribute name="plan.policy.dealer.dealerId" type="select" data="DealerList" displayorder="7" script="onChange='getEmployees(this.value)'"  label="policy.dealer"/>
		<attribute name="plan.policy.employeeId" type="select" data="EmpList" displayorder="8"  label="policy.employee" id="employees"/>
	</atts>	
	<atts name="policy">
		<attribute name="plan.products" type="checkbox" label="policy.model"/>
	</atts>
	<atts name="vehicle">
		<attribute name="plan.vehicle.regNumber" type="textfield" label="policy.regNumber"/>
		<attribute name="plan.vehicle.vinNumber" type="textfield" label="policy.vinNumber"/>
		<attribute name="plan.vehicle.purchasePrice" type="textfield" label="policy.purchasePrice"/>
	</atts>
	<atts name="customer">
		<attribute name="plan.customer.title" type="textfield" label="customer.title"/>
		<attribute name="plan.customer.lastName" type="textfield" label="customer.lastName"/>
		<attribute name="plan.customer.companyName" type="textfield" label="customer.lastName"/>
		<attribute name="plan.customer.email" type="textfield" label="customer.email"/>
		<attribute name="plan.customer.phoneNo" type="textfield" label="customer.phoneNo"/>
		<attribute name="plan.customer.mobileNo" type="textfield" label="customer.mobileNo"/>
		<attribute name="plan.customer.dateOfBirth" type="textfield" label="customer.dateOfBirth"/>
		<attribute name="plan.customer.address1" type="textfield" label="customer.address1"/>
		<attribute name="plan.customer.address2" type="textfield" label="customer.address2"/>
		<attribute name="plan.customer.address3" type="textfield" label="customer.address3"/>
		<attribute name="plan.customer.address4" type="textfield" label="customer.address4"/>
		<attribute name="plan.customer.postCode" type="textfield" label="customer.postCode"/>
	</atts>
</policy-atts>